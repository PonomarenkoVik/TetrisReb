        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Open Game" Height="450" Width="400">
    <Grid>
        <DataGrid x:Name="Table" Margin="10" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.ColumnSpan="3" MouseDoubleClick="Table_OnMouseDoubleClick" ></DataGrid>
    </Grid>
</Window>
